@import 'nib'

.member
    clearfix()
    margin 0 0 25px 0
    text-align left
    background rgba(255, 255, 255, .7)
    &__photo
        width 96px
        float left
    &__name
        overflow hidden
        margin .5em
        padding 0 0 0 20px
        font-size 21px
        line-height 1
    &__rank
        overflow hidden
        margin .5em
        padding 0 0 0 20px